do
    Tabs.Main:AddButton({
        Title = "Warp Farming spot F16",
        Callback = function()
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 1: ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
            local targetPosition = CFrame.new(-1749.17041015625, 175.7864990234375, 2772.35986328125)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPosition
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 2: ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏ï‡πà‡∏≠‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á
            spawn(function()
                local player = game.Players.LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()
                local humanoid = character:WaitForChild("Humanoid")
    
                while true do
                    if (character.HumanoidRootPart.Position - targetPosition.Position).Magnitude > 5 then
                        humanoid:MoveTo(targetPosition.Position)
                    end
                    wait(1)
                end
            end)
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 3: ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á‡πÅ‡∏•‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ
            spawn(function()
                local player = game.Players.LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()
                local detectionRadius = 2500 -- ‡∏£‡∏∞‡∏¢‡∏∞‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö
    
                local placeID = 4734865416 -- Place ID ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ
                local teleportService = game:GetService("TeleportService")
    
                local function teleportPlayer(targetPlayer)
                    teleportService:Teleport(placeID, targetPlayer)
                end
    
                while true do
                    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                        if otherPlayer.Name ~= "testerloadsss" and otherPlayer.Name ~= "DarkFalcon_Z" and otherPlayer.Name ~= "zMaxica" and otherPlayer.Name ~= "testerloadxxx" and otherPlayer ~= player
                            and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
                            local otherCharacter = otherPlayer.Character
                            local distance = (character.HumanoidRootPart.Position - otherCharacter.HumanoidRootPart.Position).Magnitude
    
                            if distance <= detectionRadius then
                                -- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÑ‡∏õ‡∏¢‡∏±‡∏á Discord Webhook
                                local OSTime = os.time()
                                local Time = os.date('!*t', OSTime)
                                local Content = 'Found a player, teleporting to a safe zone!'
                                local Embed = {
                                    title = 'Nearby Player Detected',
                                    color = 99999,
                                    footer = { text = game.JobId },
                                    author = {
                                        name = 'ROBLOX',
                                        url = 'https://www.roblox.com/',
                                    },
                                    fields = {
                                        { name = 'Teleporting...', value = 'Player is now moving to a safer place.' },
                                    },
                                    timestamp = string.format('%d-%d-%dT%02d:%02d:%02dZ', Time.year, Time.month, Time.day, Time.hour, Time.min, Time.sec),
                                }
                                (syn and syn.request or http_request) {
                                    Url = 'https://discord.com/api/webhooks/1120037043200856174/64TZOaooeaqUMb5xqDAOKxODppDYATkNhGcLPtNvB5u6D1Po_vxepEZGqeCQiOHA9Sib',
                                    Method = 'POST',
                                    Headers = { ['Content-Type'] = 'application/json' },
                                    Body = game:GetService('HttpService'):JSONEncode({ content = Content, embeds = { Embed } }),
                                }
    
                                -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á Place ID
                                print("‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á, ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á Place ID ‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î...")
                                teleportPlayer(player)
                                break
                            end
                        end
                    end
                    wait(1)
                end
            end)
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 4: ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Health Crystal
            local lastUsedTime = 0 -- ‡πÄ‡∏Å‡πá‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏™‡∏∏‡∏î‡∏ó‡πâ‡∏≤‡∏¢
            local cooldownTime = 30 -- ‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå 30 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
    
            local function checkHealthAndUseCrystal()
                local player = game.Players.LocalPlayer -- ‡πÄ‡∏Ç‡πâ‡∏≤‡∏ñ‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡πÄ‡∏£‡∏≤
                local character = player.Character or player.CharacterAdded:Wait()
                local humanoid = character:FindFirstChild("Humanoid") -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Humanoid
    
                if humanoid then
                    local currentHealth = humanoid.Health -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
                    local maxHealth = humanoid.MaxHealth -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î
    
                    -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 20% ‡πÅ‡∏•‡∏∞‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏ï‡∏¥‡∏î Cooldown
                    if currentHealth / maxHealth <= 0.2 then
                        local currentTime = os.time() -- ‡πÄ‡∏ß‡∏•‡∏≤‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
                        if currentTime - lastUsedTime >= cooldownTime then
                            -- ‡πÉ‡∏ä‡πâ Health Crystal
                            local args = {
                                [1] = "Health Crystal"
                            }
                            game:GetService("ReplicatedStorage").UseConsumable:InvokeServer(unpack(args))
    
                            -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
                            lastUsedTime = currentTime
                            print("Health Crystal used!")
                        else
                            print("Health Crystal is on cooldown.")
                        end
                    end
                end
            end
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ Health Crystal ‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
            game:GetService("RunService").Heartbeat:Connect(checkHealthAndUseCrystal)
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 5: ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡πá‡∏ô Admin ‡πÅ‡∏•‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á PlaceID
            local AdminList = {3391044573, 1258519, 5352128, 8319210, 10512120, 17637379, 18436454, 18951266, 21877984, 27513204, 28822405, 32873573, 32985263, 34923509, 36104793, 36952262, 37613772, 37784616, 43839070, 46478188, 48921674, 58736484, 64219597, 70926245, 85430726, 88031195, 90504399, 118925802, 125287135, 125754354, 127723955, 165367193, 196467370, 207160289, 211117443, 265465705, 315063621, 394422754, 414043237, 655470167, 663289590, 682061085, 1275528885, 1277054439, 1419777109, 1509327150, 1653533349, 3097986992, 4952125890} 
            local PrintUsernameOfPlayersJoining = true
            local DestinationPlaceID = 4733278992 -- ‡πÉ‡∏™‡πà PlaceID ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ Teleport ‡πÑ‡∏õ
    
            local function SendToDiscordWebhook()
                local OSTime = os.time()
                local Time = os.date('!*t', OSTime)
                local Avatar = 'https://cdn.discordapp.com/embed/avatars/4.png'
                local Content = 'Found a Admin, teleporting to a safe game!'
                local Embed = {
                    title = 'Detected Admin in the server',
                    color = 99999,
                    footer = { text = game.JobId },
                    author = {
                        name = 'ROBLOX',
                        url = 'https://www.roblox.com/',
                    },
                    fields = {
                        {
                            name = 'Teleporting...',
                            value = 'Its time to go to new game please rejoin SBOR.',
                        }
                    },
                    timestamp = string.format('%d-%d-%dT%02d:%02d:%02dZ', Time.year, Time.month, Time.day, Time.hour, Time.min, Time.sec),
                }
    
                local requestData = {
                    Url = 'https://discord.com/api/webhooks/1120037043200856174/64TZOaooeaqUMb5xqDAOKxODppDYATkNhGcLPtNvB5u6D1Po_vxepEZGqeCQiOHA9Sib',
                    Method = 'POST',
                    Headers = { ['Content-Type'] = 'application/json' },
                    Body = game:GetService('HttpService'):JSONEncode({ content = Content, embeds = { Embed } }),
                }
    
                (syn and syn.request or http_request)(requestData)
            end
    
            local function TeleportToPlace(player)
                local TeleportService = game:GetService("TeleportService")
                local success, err = pcall(function()
                    TeleportService:Teleport(DestinationPlaceID, player)
                end)
    
                if not success then
                    warn("Failed to teleport player: " .. err)
                end
            end
    
            game.Players.PlayerAdded:Connect(function(player)
                if PrintUsernameOfPlayersJoining then
                    print(player.Name .. " has joined the game")
                end
    
                if table.find(AdminList, player.UserId) then
                    SendToDiscordWebhook() -- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÑ‡∏õ‡∏¢‡∏±‡∏á Discord webhook ‡∏Å‡πà‡∏≠‡∏ô teleport
                    TeleportToPlace(player)
                end
            end)
        end
    })
    

    Tabs.Main:AddButton({
        Title = "Detect Player & Heal",
        Callback = function()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local detectionRadius = 700 -- ‡∏£‡∏∞‡∏¢‡∏∞‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö
    
            local placeID = 4734865416 -- Place ID ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ó‡∏±‡πà‡∏ß‡πÑ‡∏õ
            local teleportService = game:GetService("TeleportService")
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
            local function teleportPlayer(targetPlayer, targetPlaceID)
                teleportService:Teleport(targetPlaceID, targetPlayer)
            end
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á
            local function checkForNearbyPlayers()
                for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                    -- ‡∏¢‡∏Å‡πÄ‡∏ß‡πâ‡∏ô‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÑ‡∏°‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö
                    if otherPlayer.Name ~= "testerloadsss" and otherPlayer.Name ~= "DarkFalcon_Z" and otherPlayer.Name ~= "zMaxica" and otherPlayer.Name ~= "testerloadxxx" and otherPlayer ~= player
                       and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        local otherCharacter = otherPlayer.Character
                        local distance = (character.HumanoidRootPart.Position - otherCharacter.HumanoidRootPart.Position).Magnitude
    
                        if distance <= detectionRadius then
                            -- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÑ‡∏õ‡∏¢‡∏±‡∏á Discord Webhook
                            local OSTime = os.time()
                            local Time = os.date('!*t', OSTime)
                            local Avatar = 'https://cdn.discordapp.com/embed/avatars/4.png'
                            local Content = 'Found a player, teleporting to a safe zone!'
                            local Embed = {
                                title = 'Nearby Player Detected',
                                color = 99999,
                                footer = { text = game.JobId },
                                author = {
                                    name = 'ROBLOX',
                                    url = 'https://www.roblox.com/',
                                },
                                fields = {
                                    {
                                        name = 'Teleporting...',
                                        value = 'Player is now moving to a safer place.',
                                    }
                                },
                                timestamp = string.format('%d-%d-%dT%02d:%02d:%02dZ', Time.year, Time.month, Time.day, Time.hour, Time.min, Time.sec),
                            }
                            (syn and syn.request or http_request) {
                                Url = 'https://discord.com/api/webhooks/1120037043200856174/64TZOaooeaqUMb5xqDAOKxODppDYATkNhGcLPtNvB5u6D1Po_vxepEZGqeCQiOHA9Sib',
                                Method = 'POST',
                                Headers = { ['Content-Type'] = 'application/json' },
                                Body = game:GetService('HttpService'):JSONEncode({ content = Content; embeds = { Embed } }),
                            }
    
                            -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á Place ID
                            print("‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á, ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á Place ID ‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î...")
                            teleportPlayer(player, placeID)
                            break
                        end
                    end
                end
            end
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö Admin
            local AdminList = {3391044573, 1258519, 5352128, 8319210, 10512120, 17637379, 18436454, 18951266, 21877984, 27513204, 28822405, 32873573, 32985263, 34923509, 36104793, 36952262, 37613772, 37784616, 43839070, 46478188, 48921674, 58736484, 64219597, 70926245, 85430726, 88031195, 90504399, 118925802, 125287135, 125754354, 127723955, 165367193, 196467370, 207160289, 211117443, 265465705, 315063621, 394422754, 414043237, 655470167, 663289590, 682061085, 1275528885, 1277054439, 1419777109, 1509327150, 1653533349, 3097986992, 4952125890}
            local DestinationPlaceID = 4733278992
    
            local function checkForAdminPlayers()
                for _, admin in pairs(game.Players:GetPlayers()) do
                    if table.find(AdminList, admin.UserId) then
                        local OSTime = os.time()
                        local Time = os.date('!*t', OSTime)
                        local Embed = {
                            title = 'Detected Admin in the server',
                            color = 99999,
                            footer = { text = game.JobId },
                            author = { name = 'ROBLOX', url = 'https://www.roblox.com/' },
                            fields = { { name = 'Teleporting...', value = 'Admin detected. Teleporting to safe place.' } },
                            timestamp = string.format('%d-%d-%dT%02d:%02d:%02dZ', Time.year, Time.month, Time.day, Time.hour, Time.min, Time.sec),
                        }
                        (syn and syn.request or http_request) {
                            Url = 'https://discord.com/api/webhooks/1120037043200856174/64TZOaooeaqUMb5xqDAOKxODppDYATkNhGcLPtNvB5u6D1Po_vxepEZGqeCQiOHA9Sib',
                            Method = 'POST',
                            Headers = { ['Content-Type'] = 'application/json' },
                            Body = game:GetService('HttpService'):JSONEncode({ embeds = { Embed } }),
                        }
                        -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏≠‡∏î‡∏°‡∏¥‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á DestinationPlaceID
                        teleportPlayer(admin, DestinationPlaceID)
                        break
                    end
                end
            end
    
            -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Health Crystal
            local lastUsedTime = 0
            local cooldownTime = 30
    
            local function checkHealthAndUseCrystal()
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    local currentHealth = humanoid.Health
                    local maxHealth = humanoid.MaxHealth
                    if currentHealth / maxHealth <= 0.2 and os.time() - lastUsedTime >= cooldownTime then
                        game:GetService("ReplicatedStorage").UseConsumable:InvokeServer("Health Crystal")
                        lastUsedTime = os.time()
                        print("Health Crystal used!")
                    end
                end
            end
    
            -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
            player.CharacterAdded:Connect(function(newCharacter)
                character = newCharacter
            end)
    
            -- ‡∏•‡∏π‡∏õ‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö
            while wait(0.5) do
                checkForNearbyPlayers()
                checkForAdminPlayers()
                checkHealthAndUseCrystal()
            end
        end
    })
    
    
    
-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏ó‡∏µ‡πà‡πÄ‡∏Å‡πá‡∏ö‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞‡∏Å‡∏≤‡∏£‡πÉ‡∏ä‡πâ Health Crystal
local lastUsedTime = 0 -- ‡πÄ‡∏Å‡πá‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏™‡∏∏‡∏î‡∏ó‡πâ‡∏≤‡∏¢
local cooldownTime = 30 -- ‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå 30 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Health Crystal
local function checkHealthAndUseCrystal()
    local player = game.Players.LocalPlayer -- ‡πÄ‡∏Ç‡πâ‡∏≤‡∏ñ‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡πÄ‡∏£‡∏≤
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:FindFirstChild("Humanoid") -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Humanoid
    
    if humanoid then
        local currentHealth = humanoid.Health -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
        local maxHealth = humanoid.MaxHealth -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î
        
        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 20% ‡πÅ‡∏•‡∏∞‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏ï‡∏¥‡∏î Cooldown
        if currentHealth / maxHealth <= 0.2 then
            local currentTime = os.time() -- ‡πÄ‡∏ß‡∏•‡∏≤‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
            if currentTime - lastUsedTime >= cooldownTime then
                -- ‡πÉ‡∏ä‡πâ Health Crystal
                local args = {
                    [1] = "Health Crystal"
                }
                game:GetService("ReplicatedStorage").UseConsumable:InvokeServer(unpack(args))
                
                -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
                lastUsedTime = currentTime
                print("Health Crystal used!")
            else
                print("Health Crystal is on cooldown.")
            end
        end
    end
end

-- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï CD ‡πÄ‡∏°‡∏∑‡πà‡∏≠ Character ‡πÉ‡∏´‡∏°‡πà‡∏ñ‡∏π‡∏Å‡∏™‡∏£‡πâ‡∏≤‡∏á (‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà)
local player = game.Players.LocalPlayer
player.CharacterAdded:Connect(function(character)
    -- ‡∏£‡∏µ‡πÄ‡∏ã‡πá‡∏ï‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå
    lastUsedTime = 0
    print("Player respawned. Cooldown reset.")
end)

-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏õ‡∏∏‡πà‡∏°‡πÉ‡∏ô UI
Tabs.Main:AddButton({
    Title = "Auto Heal",  -- ‡∏ä‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°
    Callback = function()
        -- ‡πÉ‡∏ä‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ô‡∏µ‡πâ‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏õ‡∏∏‡πà‡∏°‡∏ñ‡∏π‡∏Å‡∏Å‡∏î
        game:GetService("RunService").Heartbeat:Connect(function()
            checkHealthAndUseCrystal()
        end)
    end
})

Tabs.Main:AddButton({
    Title = "Warp Farming Floor 8",
    Callback = function()
        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 1: ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
        local targetPosition = CFrame.new(292.79608154296875, 58.900856018066406, -1424.84130859375)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetPosition

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 2: ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏ï‡πà‡∏≠‡πÄ‡∏ô‡∏∑‡πà‡∏≠‡∏á
        spawn(function()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:WaitForChild("Humanoid")

            while true do
                if (character.HumanoidRootPart.Position - targetPosition.Position).Magnitude > 5 then
                    humanoid:MoveTo(targetPosition.Position)
                end
                wait(1)
            end
        end)

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 3: ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á‡πÅ‡∏•‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ
        spawn(function()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local detectionRadius = 3500 -- ‡∏£‡∏∞‡∏¢‡∏∞‡∏Å‡∏≤‡∏£‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö

            local placeID = 4734865416 -- Place ID ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ
            local teleportService = game:GetService("TeleportService")

            local function teleportPlayer(targetPlayer)
                teleportService:Teleport(placeID, targetPlayer)
            end

            while true do
                for _, otherPlayer in pairs(game.Players:GetPlayers()) do
                    if otherPlayer.Name ~= "testerloadsss" and otherPlayer.Name ~= "DarkFalcon_Z" and otherPlayer.Name ~= "zMaxica" and otherPlayer.Name ~= "testerloadxxx" and otherPlayer ~= player
                        and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
                        local otherCharacter = otherPlayer.Character
                        local distance = (character.HumanoidRootPart.Position - otherCharacter.HumanoidRootPart.Position).Magnitude

                        if distance <= detectionRadius then
                            -- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÑ‡∏õ‡∏¢‡∏±‡∏á Discord Webhook
                            local OSTime = os.time()
                            local Time = os.date('!*t', OSTime)
                            local Content = 'Found a player, teleporting to a safe zone!'
                            local Embed = {
                                title = 'Nearby Player Detected',
                                color = 99999,
                                footer = { text = game.JobId },
                                author = {
                                    name = 'ROBLOX',
                                    url = 'https://www.roblox.com/',
                                },
                                fields = {
                                    { name = 'Teleporting...', value = 'Player is now moving to a safer place.' },
                                },
                                timestamp = string.format('%d-%d-%dT%02d:%02d:%02dZ', Time.year, Time.month, Time.day, Time.hour, Time.min, Time.sec),
                            }
                            (syn and syn.request or http_request) {
                                Url = 'https://discord.com/api/webhooks/1120037043200856174/64TZOaooeaqUMb5xqDAOKxODppDYATkNhGcLPtNvB5u6D1Po_vxepEZGqeCQiOHA9Sib',
                                Method = 'POST',
                                Headers = { ['Content-Type'] = 'application/json' },
                                Body = game:GetService('HttpService'):JSONEncode({ content = Content, embeds = { Embed } }),
                            }

                            -- ‡∏ß‡∏≤‡∏£‡πå‡∏õ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÑ‡∏õ‡∏¢‡∏±‡∏á Place ID
                            print("‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏Ñ‡∏µ‡∏¢‡∏á, ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á Place ID ‡∏ó‡∏µ‡πà‡∏Å‡∏≥‡∏´‡∏ô‡∏î...")
                            teleportPlayer(player)
                            break
                        end
                    end
                end
                wait(1)
            end
        end)

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 4: ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Health Crystal
        local lastUsedTime = 0 -- ‡πÄ‡∏Å‡πá‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏™‡∏∏‡∏î‡∏ó‡πâ‡∏≤‡∏¢
        local cooldownTime = 30 -- ‡∏£‡∏∞‡∏¢‡∏∞‡πÄ‡∏ß‡∏•‡∏≤‡∏Ñ‡∏π‡∏•‡∏î‡∏≤‡∏ß‡∏ô‡πå 30 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ

        local function checkHealthAndUseCrystal()
            local player = game.Players.LocalPlayer -- ‡πÄ‡∏Ç‡πâ‡∏≤‡∏ñ‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏Ç‡∏≠‡∏á‡πÄ‡∏£‡∏≤
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoid = character:FindFirstChild("Humanoid") -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö Humanoid

            if humanoid then
                local currentHealth = humanoid.Health -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
                local maxHealth = humanoid.MaxHealth -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏™‡∏π‡∏á‡∏™‡∏∏‡∏î

                -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ß‡πà‡∏≤‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡∏ï‡πà‡∏≥‡∏Å‡∏ß‡πà‡∏≤ 20% ‡πÅ‡∏•‡∏∞‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏ï‡∏¥‡∏î Cooldown
                if currentHealth / maxHealth <= 0.2 then
                    local currentTime = os.time() -- ‡πÄ‡∏ß‡∏•‡∏≤‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
                    if currentTime - lastUsedTime >= cooldownTime then
                        -- ‡πÉ‡∏ä‡πâ Health Crystal
                        local args = {
                            [1] = "Health Crystal"
                        }
                        game:GetService("ReplicatedStorage").UseConsumable:InvokeServer(unpack(args))

                        -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡πÄ‡∏ß‡∏•‡∏≤‡∏ó‡∏µ‡πà‡πÉ‡∏ä‡πâ‡∏•‡πà‡∏≤‡∏™‡∏∏‡∏î
                        lastUsedTime = currentTime
                        print("Health Crystal used!")
                    else
                        print("Health Crystal is on cooldown.")
                    end
                end
            end
        end

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÄ‡∏•‡∏∑‡∏≠‡∏î‡πÅ‡∏•‡∏∞‡πÉ‡∏ä‡πâ Health Crystal ‡∏ó‡∏∏‡∏Å 1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ
        game:GetService("RunService").Heartbeat:Connect(checkHealthAndUseCrystal)

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà 5: ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏õ‡πá‡∏ô Admin ‡πÅ‡∏•‡∏∞‡∏ß‡∏≤‡∏£‡πå‡∏õ‡πÑ‡∏õ‡∏¢‡∏±‡∏á PlaceID
        local AdminList = {3391044573, 1258519, 5352128, 8319210, 10512120, 17637379, 18436454, 18951266, 21877984, 27513204, 28822405, 32873573, 32985263, 34923509, 36104793, 36952262, 37613772, 37784616, 43839070, 46478188, 48921674, 58736484, 64219597, 70926245, 85430726, 88031195, 90504399, 118925802, 125287135, 125754354, 127723955, 165367193, 196467370, 207160289, 211117443, 265465705, 315063621, 394422754, 414043237, 655470167, 663289590, 682061085, 1275528885, 1277054439, 1419777109, 1509327150, 1653533349, 3097986992, 4952125890} 
        local PrintUsernameOfPlayersJoining = true
        local DestinationPlaceID = 4733278992 -- ‡πÉ‡∏™‡πà PlaceID ‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡πÉ‡∏´‡πâ Teleport ‡πÑ‡∏õ

        local function SendToDiscordWebhook()
            local OSTime = os.time()
            local Time = os.date('!*t', OSTime)
            local Avatar = 'https://cdn.discordapp.com/embed/avatars/4.png'
            local Content = 'Found a Admin, teleporting to a safe game!'
            local Embed = {
                title = 'Detected Admin in the server',
                color = 99999,
                footer = { text = game.JobId },
                author = {
                    name = 'ROBLOX',
                    url = 'https://www.roblox.com/',
                },
                fields = {
                    {
                        name = 'Teleporting...',
                        value = 'Its time to go to new game please rejoin SBOR.',
                    }
                },
                timestamp = string.format('%d-%d-%dT%02d:%02d:%02dZ', Time.year, Time.month, Time.day, Time.hour, Time.min, Time.sec),
            }

            local requestData = {
                Url = 'https://discord.com/api/webhooks/1120037043200856174/64TZOaooeaqUMb5xqDAOKxODppDYATkNhGcLPtNvB5u6D1Po_vxepEZGqeCQiOHA9Sib',
                Method = 'POST',
                Headers = { ['Content-Type'] = 'application/json' },
                Body = game:GetService('HttpService'):JSONEncode({ content = Content, embeds = { Embed } }),
            }

            (syn and syn.request or http_request)(requestData)
        end

        local function TeleportToPlace(player)
            local TeleportService = game:GetService("TeleportService")
            local success, err = pcall(function()
                TeleportService:Teleport(DestinationPlaceID, player)
            end)

            if not success then
                warn("Failed to teleport player: " .. err)
            end
        end

        game.Players.PlayerAdded:Connect(function(player)
            if PrintUsernameOfPlayersJoining then
                print(player.Name .. " has joined the game")
            end

            if table.find(AdminList, player.UserId) then
                SendToDiscordWebhook() -- ‡∏™‡πà‡∏á‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÑ‡∏õ‡∏¢‡∏±‡∏á Discord webhook ‡∏Å‡πà‡∏≠‡∏ô teleport
                TeleportToPlace(player)
            end
        end)
    end
})


local Input = Tabs.Settings:AddInput("Input", {
        Title = "Input",
        Default = "Default",
        Placeholder = "Placeholder",
        Numeric = false, -- Only allows numbers
        Finished = false, -- Only calls callback when you press enter
        Callback = function(Value)
            print("Input changed:", Value)
        end
    })

    Input:OnChanged(function()
        print("Input updated:", Input.Value)
    end)
end


-- ‡∏™‡∏£‡πâ‡∏≤‡∏á‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ö‡∏±‡∏ç‡∏ä‡∏µ‡∏´‡∏•‡∏±‡∏Å
Tabs.Teleport:AddButton({
    Title = "TP to main account",
    Callback = function()
        local targetPlayerName = "zMaxica"
        local player = game.Players.LocalPlayer

        -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡∏≠‡∏á "zMaxica"
        local function teleportToTargetPlayer()
            local targetPlayer = game.Players:FindFirstChild(targetPlayerName)

            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetCharacter = targetPlayer.Character
                local targetPosition = targetCharacter.HumanoidRootPart.Position

                -- ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô "zMaxica"
                player.Character:SetPrimaryPartCFrame(CFrame.new(targetPosition))
            else
                print("‡πÑ‡∏°‡πà‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡∏ó‡∏µ‡πà‡∏ä‡∏∑‡πà‡∏≠ " .. targetPlayerName)
            end
        end

        -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡πÅ‡∏£‡∏Å‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Å‡∏î‡∏õ‡∏∏‡πà‡∏°
        teleportToTargetPlayer()

        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÅ‡∏•‡∏∞‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô "zMaxica" ‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
        player.CharacterAdded:Connect(function(character)
            -- ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÉ‡∏´‡∏°‡πà‡∏à‡∏∞‡∏°‡∏µ HumanoidRootPart
            local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
            
            -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡πÅ‡∏•‡πâ‡∏ß‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô "zMaxica"
            wait(0.5) -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏û‡∏£‡πâ‡∏≠‡∏°‡πÅ‡∏•‡πâ‡∏ß‡∏Ñ‡πà‡∏≠‡∏¢‡∏ó‡∏≥‡∏Å‡∏≤‡∏£‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï

            teleportToTargetPlayer()
        end)
    end
})

