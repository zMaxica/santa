ImageButton.Size = UDim2.new(0, 120, 0, 120)
ImageButton.Image = "rbxassetid://95636384330694"  -- ‡πÉ‡∏™‡πà Asset ID ‡∏Ç‡∏≠‡∏á‡∏£‡∏π‡∏õ‡∏†‡∏≤‡∏û‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏Ñ‡∏•‡∏¥‡∏Å ImageButton
ImageButton.MouseButton1Down:Connect(function()
    -- ‡∏™‡πà‡∏á‡∏Å‡∏≤‡∏£‡∏Å‡∏î‡∏Ñ‡∏µ‡∏¢‡πå LeftControl
    game:GetService("VirtualInputManager"):SendKeyEvent(true, "F6", false, game)
end)



local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "[üíÄ] StayCools ",
    SubTitle = "But SleepyUncle",
    TabWidth = 160,
    Size = UDim2.fromOffset(480, 360),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur aentirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.F6 -- Used when theres no MinimizeKeybind
})

--Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
local Tabs = {
    Main = Window:AddTab({ Title = "Auto Farm", Icon = "play" }),
    Teleport = Window:AddTab({ Title = "Teleport floor 1-10", Icon = "plane" }),
    Teleport2 = Window:AddTab({ Title = "Teleport floor 11-17", Icon = "plane" }),
    Changeserver = Window:AddTab({ Title = "Change Server", Icon = "rss" }),
    Function = Window:AddTab({ Title = "Function", Icon = "activity" }),
    FindPosition = Window:AddTab({ Title = "Find Position", Icon = "locate" }),
    EventCornMaze = Window:AddTab({ Title = "Event Corn Maze", Icon = "ghost" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}
local Options = Fluent.Options
Window:SelectTab(1)
