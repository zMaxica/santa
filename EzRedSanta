local Toggle = Tabs.Function:AddToggle("SpeedWalkerToggle", { 
    Title = "Speed Walker",
    Default = false, -- ‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
    Callback = function(state)
        local character = game.Players.LocalPlayer.Character
        if character and character:FindFirstChild("Humanoid") then
            if state then
                character.Humanoid.WalkSpeed = 150
            else
                character.Humanoid.WalkSpeed = 16
            end
        else
            warn("Humanoid not found!")
        end
    end
})

Toggle:OnChanged(function()
    print("Toggle changed:", Options.SpeedWalkerToggle.Value)
end)

Options.SpeedWalkerToggle:SetValue(false) -- ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏Ñ‡πà‡∏≤‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô

Tabs.Function:AddButton({
    Title = "INF JUMP",
    Description = "Enable Infinite Jump",
    Callback = function()
        local InfiniteJumpEnabled = true
        game:GetService("UserInputService").JumpRequest:Connect(function()
            if InfiniteJumpEnabled then
                game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
            end
        end)
    end
})

Tabs.Function:AddButton({
    Title = "Auto get sword",
    Description = "Enable Infinite Jump",
    Callback = function()
        -- Services
local virtualInputManager = game:GetService("VirtualInputManager")
local player = game.Players.LocalPlayer

-- ‡∏û‡∏¥‡∏Å‡∏±‡∏î‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏õ‡∏∏‡πà‡∏°‡∏ö‡∏ô‡∏´‡∏ô‡πâ‡∏≤‡∏à‡∏≠
local targetPosition = Vector2.new(820.086, 500.407)

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏à‡∏≥‡∏•‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏Ñ‡∏•‡∏¥‡∏Å
local function simulateMouseClick()
    -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏°‡∏≤‡∏™‡πå‡∏ó‡∏µ‡πà‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
    virtualInputManager:SendMouseButtonEvent(
        targetPosition.X, -- ‡∏û‡∏¥‡∏Å‡∏±‡∏î X
        targetPosition.Y, -- ‡∏û‡∏¥‡∏Å‡∏±‡∏î Y
        0,               -- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏°‡∏≤‡∏™‡πå (0 = ‡∏ã‡πâ‡∏≤‡∏¢, 1 = ‡∏Ç‡∏ß‡∏≤)
        true,            -- ‡∏Ñ‡∏•‡∏¥‡∏Å‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ï‡πâ‡∏ô
        game,            -- ‡∏≠‡πâ‡∏≤‡∏á‡∏≠‡∏¥‡∏á‡πÄ‡∏Å‡∏°
        0                -- ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Å‡∏≤‡∏£‡∏Å‡∏≥‡∏´‡∏ô‡∏î InputObject
    )

    -- ‡∏õ‡∏•‡πà‡∏≠‡∏¢‡∏Ñ‡∏•‡∏¥‡∏Å
    virtualInputManager:SendMouseButtonEvent(
        targetPosition.X,
        targetPosition.Y,
        0,
        false, -- ‡∏Ñ‡∏•‡∏¥‡∏Å‡∏™‡∏¥‡πâ‡∏ô‡∏™‡∏∏‡∏î
        game,
        0
    )
end

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
local function onCharacterAdded()
    -- ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡πÇ‡∏´‡∏•‡∏î‡πÄ‡∏™‡∏£‡πá‡∏à‡∏™‡∏°‡∏ö‡∏π‡∏£‡∏ì‡πå
    wait(2) 

    -- ‡∏£‡∏±‡∏ô‡∏Ñ‡∏•‡∏¥‡∏Å 10 ‡∏£‡∏≠‡∏ö ‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏£‡∏≠ 0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡πÉ‡∏ô‡πÅ‡∏ï‡πà‡∏•‡∏∞‡∏£‡∏≠‡∏ö
    for i = 1, 7 do 
        simulateMouseClick()
        wait(0.1) -- ‡∏£‡∏≠ 0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ‡∏Å‡πà‡∏≠‡∏ô‡∏Ñ‡∏•‡∏¥‡∏Å‡∏£‡∏≠‡∏ö‡∏ñ‡∏±‡∏î‡πÑ‡∏õ
    end
end

-- ‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡∏Å‡∏±‡∏ö Event ‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡∏Ç‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£
player.CharacterAdded:Connect(onCharacterAdded)

-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏´‡∏≤‡∏Å‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà‡πÅ‡∏•‡πâ‡∏ß
if player.Character then
    onCharacterAdded()
end 
    end
})

Tabs.Function:AddButton({
    Title = "USE CANDY",
    Description = "Consume Candy",
    Callback = function()
        while true do
            local args = { [1] = "Candy" }
            game:GetService("ReplicatedStorage").UseConsumable:InvokeServer(unpack(args))
            wait()
        end
    end
})

Tabs.Function:AddButton({
    Title = "Auto Material",
    Description = "Automatically collect materials",
    Callback = function()
        local function collectMaterial(material)
            if material:FindFirstChild("Id") and material:FindFirstChild("Owner") and material.Owner.Value == "" then
                local matID = material.Id.Value
                local args = {matID}
                pcall(function()
                    game:GetService("ReplicatedStorage").ClaimMaterial:InvokeServer(unpack(args))
                    print("Collected Material:", material.Name, "ID:", matID)
                end)
            else
                print("Skipped:", material.Name, "- Owned or No ID.")
            end
        end

        -- ‡∏•‡∏π‡∏õ‡πÄ‡∏Å‡πá‡∏ö Material ‡πÅ‡∏ö‡∏ö‡πÑ‡∏°‡πà‡∏£‡∏π‡πâ‡∏à‡∏ö
        task.spawn(function()
            while true do
                -- ‡∏ß‡∏ô‡∏•‡∏π‡∏õ‡πÄ‡∏Å‡πá‡∏ö Material ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏ó‡∏µ‡πà‡∏°‡∏µ‡∏≠‡∏¢‡∏π‡πà
                for _, material in ipairs(workspace.Materials:GetChildren()) do
                    collectMaterial(material)
                end
                -- ‡∏£‡∏≠ 0.1 ‡∏ß‡∏¥‡∏ô‡∏≤‡∏ó‡∏µ ‡∏Å‡πà‡∏≠‡∏ô‡∏ß‡∏ô‡∏•‡∏π‡∏õ‡∏£‡∏≠‡∏ö‡∏ñ‡∏±‡∏î‡πÑ‡∏õ
                task.wait(0.1)
            end
        end)

        -- ‡πÄ‡∏Å‡πá‡∏ö Material ‡πÉ‡∏´‡∏°‡πà‡πÄ‡∏°‡∏∑‡πà‡∏≠‡∏ñ‡∏π‡∏Å‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤‡πÉ‡∏ô workspace
        workspace.Materials.ChildAdded:Connect(collectMaterial)
    end
})

-- ‡∏ï‡∏±‡∏ß‡πÅ‡∏õ‡∏£‡πÄ‡∏Å‡πá‡∏ö‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á CFrame ‡∏ó‡∏µ‡πà‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å
local copiedPosition = nil

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á CFrame
Tabs.FindPosition:AddButton({
    Title = "Get CFrame",
    Callback = function()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            copiedPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
            setclipboard(tostring(copiedPosition)) -- ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏Ñ‡πà‡∏≤‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏Ñ‡∏•‡∏¥‡∏õ‡∏ö‡∏≠‡∏£‡πå‡∏î
            print("CFrame copied to clipboard: " .. tostring(copiedPosition))
        else
            warn("HumanoidRootPart not found!")
        end
    end
})

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
Tabs.FindPosition:AddButton({
    Title = "Copy & Auto Move to Position",
    Callback = function()
        -- ‡∏Ç‡∏±‡πâ‡∏ô‡∏ï‡∏≠‡∏ô‡∏ó‡∏µ‡πà 1: ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            copiedPosition = player.Character.HumanoidRootPart.Position
            setclipboard(tostring(copiedPosition)) -- ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏Ñ‡πà‡∏≤‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏Ñ‡∏•‡∏¥‡∏õ‡∏ö‡∏≠‡∏£‡πå‡∏î
            print("Position copied to clipboard: " .. tostring(copiedPosition))
        else
            warn("HumanoidRootPart not found!")
            return -- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏´‡∏≤‡∏Å‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
        end

        -- ‡∏Ç‡∏±‡πâ‡∏ô‡∏ï‡∏≠‡∏ô‡∏ó‡∏µ‡πà 2: ‡∏ï‡∏±‡πâ‡∏á‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
        local function moveToCopiedPosition(character)
            if copiedPosition and character then
                local humanoid = character:WaitForChild("Humanoid")
                local rootPart = character:WaitForChild("HumanoidRootPart")

                -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
                while copiedPosition do
                    if (rootPart.Position - copiedPosition).Magnitude > 0 then
                        humanoid:MoveTo(copiedPosition)
                    end
                    wait(1)
                end
            else
                warn("Failed to move: Missing position or character.")
            end
        end

        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
        player.CharacterAdded:Connect(function(character)
            moveToCopiedPosition(character)
        end)

        -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
        if player.Character then
            moveToCopiedPosition(player.Character)
        end
    end
})

-- ‡∏õ‡∏∏‡πà‡∏°‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
Tabs.FindPosition:AddButton({
    Title = "Auto Stand but can die",
    Callback = function()
        -- ‡∏Ç‡∏±‡πâ‡∏ô‡∏ï‡∏≠‡∏ô‡∏ó‡∏µ‡πà 1: ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            copiedPosition = player.Character.HumanoidRootPart.Position
            setclipboard(tostring(copiedPosition)) -- ‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å‡∏Ñ‡πà‡∏≤‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏Ñ‡∏•‡∏¥‡∏õ‡∏ö‡∏≠‡∏£‡πå‡∏î
            print("Position copied to clipboard: " .. tostring(copiedPosition))
        else
            warn("HumanoidRootPart not found!")
            return -- ‡∏´‡∏¢‡∏∏‡∏î‡∏Å‡∏≤‡∏£‡∏ó‡∏≥‡∏á‡∏≤‡∏ô‡∏´‡∏≤‡∏Å‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á
        end

        -- ‡∏Ç‡∏±‡πâ‡∏ô‡∏ï‡∏≠‡∏ô‡∏ó‡∏µ‡πà 2: ‡∏ï‡∏±‡πâ‡∏á‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
        local function moveToCopiedPosition(character)
            if copiedPosition and character then
                local humanoid = character:WaitForChild("Humanoid")
                local rootPart = character:WaitForChild("HumanoidRootPart")

                -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
                while copiedPosition do
                    if (rootPart.Position - copiedPosition).Magnitude > 0 then
                        humanoid:MoveTo(copiedPosition)
                    end
                    wait(1)
                end
            else
                warn("Failed to move: Missing position or character.")
            end
        end

        -- ‡∏Ç‡∏±‡πâ‡∏ô‡∏ï‡∏≠‡∏ô‡∏ó‡∏µ‡πà 3: ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î‡πÑ‡∏õ‡∏¢‡∏±‡∏á‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡∏ó‡∏µ‡πà‡∏Ñ‡∏±‡∏î‡∏•‡∏≠‡∏Å
        local function setSpawnToCopiedPosition()
            if copiedPosition then
                -- ‡∏™‡∏£‡πâ‡∏≤‡∏á SpawnLocation ‡πÉ‡∏´‡∏°‡πà
                local newSpawn = Instance.new("SpawnLocation")
                newSpawn.Position = copiedPosition
                newSpawn.Size = Vector3.new(0, 0, 0)
                newSpawn.Anchored = true
                newSpawn.Parent = workspace -- ‡πÄ‡∏û‡∏¥‡πà‡∏° SpawnLocation ‡πÑ‡∏õ‡∏ó‡∏µ‡πà Workspace

                -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏≠‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô
                player.RespawnLocation = newSpawn
                print("Spawn location set to: " .. tostring(copiedPosition))
            else
                warn("No position copied yet!")
            end
        end

        -- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô‡∏Å‡∏≤‡∏£‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î
        setSpawnToCopiedPosition()

        -- ‡∏ï‡∏£‡∏ß‡∏à‡∏à‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà‡πÅ‡∏•‡∏∞‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
        player.CharacterAdded:Connect(function(character)
            -- ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏à‡∏∏‡∏î‡πÄ‡∏Å‡∏¥‡∏î‡∏Ç‡∏≠‡∏á‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
            if copiedPosition then
                local rootPart = character:WaitForChild("HumanoidRootPart")
                rootPart.CFrame = CFrame.new(copiedPosition)
            end

            -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà
            moveToCopiedPosition(character)
        end)

        -- ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡∏ó‡∏±‡∏ô‡∏ó‡∏µ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡∏ï‡∏±‡∏ß‡∏•‡∏∞‡∏Ñ‡∏£‡∏õ‡∏±‡∏à‡∏à‡∏∏‡∏ö‡∏±‡∏ô
        if player.Character then
            moveToCopiedPosition(player.Character)
        end
    end
})


-- ‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏õ‡∏∏‡πà‡∏°‡∏ï‡πà‡∏≤‡∏á‡πÜ ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÉ‡∏ô EventCornMaze Tab

local Locations = {
    {"Fire Mage", CFrame.new(648.2756958007812, 2.793100357055664, -663.1729125976562)},
    {"Earth Mage", CFrame.new(1195.9481201171875, 3.2384414672851562, -329.3531494140625)},
    {"Necro Mage", CFrame.new(1297.7694091796875, 4.663743495941162, 351.5694885253906)},
    {"ICE Mage", CFrame.new(353.7503662109375, 2.2144837379455566, 485.21087646484375)},
    {"Floor Boss", CFrame.new(712.6687622070312, 1.6562496423721313, 214.00831604003906)},
    {"Portal", CFrame.new(838.4889526367188, 6.632835865020752, 95.0987548828125)}
}

local Values = {}
for _, location in ipairs(Locations) do
    table.insert(Values, location[1])  -- ‡πÉ‡∏™‡πà‡∏ä‡∏∑‡πà‡∏≠‡∏™‡∏ñ‡∏≤‡∏ô‡∏ó‡∏µ‡πà‡∏•‡∏á‡πÉ‡∏ô‡∏£‡∏≤‡∏¢‡∏Å‡∏≤‡∏£
end

local Dropdown = Tabs.EventCornMaze:AddDropdown("Teleport Locations", {
    Title = "Teleport To",
    Values = Values,
    Multi = false,
    Default = "",
})

Dropdown:OnChanged(function(selectedLocation)
    for _, location in ipairs(Locations) do
        if location[1] == selectedLocation then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = location[2]
            break
        end
    end
end)
game:GetService("VirtualInputManager"):SendKeyEvent(true, "F6", false, game)
